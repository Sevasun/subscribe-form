{
    "extends": "eslint:recommended",
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "globals": {
        "window" : true,
        "document" : true,
        "console" : true
    },
    "rules": {
        "no-console": "off"
    }
}